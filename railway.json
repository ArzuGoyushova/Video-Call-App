{
    "services": {
      "web": {
        "build": "cd client && npm install && cd ..",
        "command": "node index.js",
        "env": {
          "PORT": 3000
        },
        "routes": [
          {
            "src": "/client/(.*)",
            "dest": "/client/$1"
          },
          {
            "src": "/(.*)",
            "dest": "/$1"
          }
        ]
      }
    }
  }
  